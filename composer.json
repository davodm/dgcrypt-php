{
    "name": "davodm/dgcrypt-php",
    "description": "A simple PHP library for encryption and decryption using AES-256-CBC to be usable in different language/devices.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Dgcrypt\\": "src/"
        }
    },
    "require": {
        "php": ">=8.2",
        "ext-openssl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "authors": [
        {
            "name": "Davod",
            "email": "i@davod.me"
        }
    ],
    "scripts": {
        "test": "phpunit --configuration phpunit.xml"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
